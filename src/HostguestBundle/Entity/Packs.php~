<?php

namespace HostguestBundle\Entity;

/**
 * Packs
 */
class Packs
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $titre;

    /**
     * @var string
     */
    private $description;

    /**
     * @var float
     */
    private $prix;

    /**
     * @var integer
     */
    private $nboffre;

    /**
     * @var \HostguestBundle\Entity\Utilisateurs
     */
    private $idHote;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set titre
     *
     * @param string $titre
     *
     * @return Packs
     */
    public function setTitre($titre)
    {
        $this->titre = $titre;

        return $this;
    }

    /**
     * Get titre
     *
     * @return string
     */
    public function getTitre()
    {
        return $this->titre;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return Packs
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set prix
     *
     * @param float $prix
     *
     * @return Packs
     */
    public function setPrix($prix)
    {
        $this->prix = $prix;

        return $this;
    }

    /**
     * Get prix
     *
     * @return float
     */
    public function getPrix()
    {
        return $this->prix;
    }

    /**
     * Set nboffre
     *
     * @param integer $nboffre
     *
     * @return Packs
     */
    public function setNboffre($nboffre)
    {
        $this->nboffre = $nboffre;

        return $this;
    }

    /**
     * Get nboffre
     *
     * @return integer
     */
    public function getNboffre()
    {
        return $this->nboffre;
    }

    /**
     * Set idHote
     *
     * @param \HostguestBundle\Entity\Utilisateurs $idHote
     *
     * @return Packs
     */
    public function setIdHote(\HostguestBundle\Entity\Utilisateurs $idHote = null)
    {
        $this->idHote = $idHote;

        return $this;
    }

    /**
     * Get idHote
     *
     * @return \HostguestBundle\Entity\Utilisateurs
     */
    public function getIdHote()
    {
        return $this->idHote;
    }
}
