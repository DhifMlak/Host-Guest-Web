<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HostguestBundle\Entity\Utilisateurs" table="utilisateurs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateNaissance" type="date" column="date_naissance" nullable="false"/>
    <field name="cin" type="string" column="cin" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pseudo" type="string" column="pseudo" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="motDePasse" type="string" column="mot_de_passe" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etat" type="integer" column="etat" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="role" type="string" column="role" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="Datefinban" type="date" column="Datefinban" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
